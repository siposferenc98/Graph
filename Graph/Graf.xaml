<Window x:Class="Graph.Graf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graph"
        mc:Ignorable="d"
        Title="Graf" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Canvas x:Name="grafTabla" Grid.Row="1" Width="800">
            <ListBox Height="384" Width="153" Canvas.Left="647" x:Name="listbox"/>
        </Canvas>
        <ComboBox HorizontalAlignment="Left" Margin="114,18,0,0" VerticalAlignment="Top" Width="120" x:Name="nodeCombobox"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="451,0,0,0" VerticalAlignment="Center" Width="50" Click="mehet"/>
        <ComboBox HorizontalAlignment="Left" Margin="303,18,0,0" VerticalAlignment="Top" Width="120" x:Name="nodeCombobox1"/>
        <Label Content="Honnan" HorizontalAlignment="Left" Margin="57,14,0,0" VerticalAlignment="Top"/>
        <Label Content="Hova" HorizontalAlignment="Left" Margin="261,14,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
